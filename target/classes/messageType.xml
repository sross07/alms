	<bean id="ResultBaseMessage" class="org.alms.messages.hl7.ResultBaseMessage">
		<property name="XSDLocation"  value="d:\\aln\\ResultBase.xsd"/>
	</bean> 
	
	<bean id="ALMSResultMessage" class="org.alms.messages.hl7.ALMS">
		<property name="XSDLocation"  value="d:\\aln\\ALMSResult.xsd"/>
