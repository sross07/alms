	<bean id="CsfResultMessage" class="org.alms.messages.hl7.CsfBaseResult">
		<property name="XSDLocation"  value="d:\\aln\\NAHLNResultCSF.xsd"/>
	</bean> 
	
	<bean id="AIResultMessage" class="org.alms.messages.hl7.AvainInfluBase">
		<property name="XSDLocation"  value="d:\\aln\\ResultWSAI.xsd"/>
	</bean> 

	<bean id="SIResultMessage" class="org.alms.messages.hl7.OrderMessage">
		<property name="XSDLocation"  value="d:\\aln\\OrderBase.xsd"/>
